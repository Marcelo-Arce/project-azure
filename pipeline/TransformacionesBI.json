{
	"name": "TransformacionesBI",
	"properties": {
		"activities": [
			{
				"name": "Notebook1",
				"type": "DatabricksNotebook",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Notebooks/TransformacionesV2",
					"baseParameters": {
						"DataLakeName": {
							"value": "@pipeline().parameters.NombreDataLake",
							"type": "Expression"
						},
						"DLAccesKey": {
							"value": "@pipeline().parameters.DataLakeAccesKey",
							"type": "Expression"
						},
						"DataContainer": {
							"value": "@pipeline().parameters.dataContainer",
							"type": "Expression"
						},
						"OutputContainer": {
							"value": "@pipeline().parameters.outputcontainer",
							"type": "Expression"
						}
					}
				},
				"linkedServiceName": {
					"referenceName": "AzureDatabricks1",
					"type": "LinkedServiceReference"
				}
			}
		],
		"parameters": {
			"NombreDataLake": {
				"type": "string"
			},
			"DataLakeAccesKey": {
				"type": "string"
			},
			"dataContainer": {
				"type": "string"
			},
			"outputcontainer": {
				"type": "string"
			}
		},
		"annotations": []
	}
}