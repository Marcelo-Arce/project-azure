					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"notebookPath": "/Actualizaci√≥n_proveedor_Final",
					"baseParameters": {
						"DataLakeName": {
							"value": "@variables('NombreDataLake')",
							"type": "Expression"
						},
						"DLAccesKey": {
							"value": "@variables('DataLakeAccesKey')",
							"type": "Expression"
						},
						"DataContainer": {
							"value": "@variables('datacontainer')",
							"type": "Expression"
						},
						"OutputContainer": {
							"value": "@variables('outputcontainer')",
							"type": "Expression"
