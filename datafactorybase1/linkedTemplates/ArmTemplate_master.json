{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "datafactorybase1"
		},
		"AzureDataLakeStorageRetail_accountKey": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"accountKey\"de \"AzureDataLakeStorageRetail\""
		},
		"AzureSqlDatabaseRetail_connectionString": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"connectionString\"de \"AzureSqlDatabaseRetail\""
		},
		"AzureDataLakeStorageRetail_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datalakebase1.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datafactorybase1_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorageRetail_accountKey": {
						"value": "[parameters('AzureDataLakeStorageRetail_accountKey')]"
					},
					"AzureSqlDatabaseRetail_connectionString": {
						"value": "[parameters('AzureSqlDatabaseRetail_connectionString')]"
					},
					"AzureDataLakeStorageRetail_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorageRetail_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}